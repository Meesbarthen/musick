<%= @booking.inspect %>
<h1><%= @booking.instrument.name %></h1>
<p> <strong>From:</strong> <%= @booking.begin_date %> <strong>To:</strong> <%= @booking.end_date %></p>
<p> <strong>Total Price: </strong><%= @booking.price %></p>


<%= link_to "Edit this booking", edit_booking_path(@booking), class: "btn btn-primary"%>
<%= link_to "Cancel this booking", booking_path(@booking), method: :delete, data: { confirm: "Are you sure?" }, class: "btn btn-primary"%>
